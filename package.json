{
  "name": "chatpost",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "serve": "npm run serve:build && npm run serve:run",
    "serve:build": "vite build --ssr ./ssr/index.ts --outDir ./dist-ssr",
    "serve:run": "node ./dist-ssr/index.js",
    "clean": "rm -rf ./dist*"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.0",
    "bootstrap": "^5.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "@vitejs/plugin-vue": "^4.2.3",
    "sass": "^1.69.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
